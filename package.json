{
  "name": "@logismiko/html-pdf",
  "version": "1.0.3",
  "description": "Convert any html content or html page to PDF",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "author": "Logismiko",
  "license": "ISC",
  "dependencies": {
    "handlebars": "^4.7.8",
    "inline-css": "^4.0.3",
    "puppeteer": "^24.1.1"
  },
  "devDependencies": {
    "@types/handlebars": "^4.1.0",
    "@types/inline-css": "^3.0.3",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "keywords": [
    "html",
    "pdf",
    "nodejs",
    "puppeteer",
    "handlebars",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NarKarapetyan93/html-pdf-node.git"
  },
  "bugs": {
    "url": "https://github.com/NarKarapetyan93/html-pdf-node/issues"
  },
  "homepage": "https://github.com/NarKarapetyan93/html-pdf-node#readme",
  "engines": {
    "node": ">=14.0.0"
  }
}
